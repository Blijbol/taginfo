function page_init() {
    up = function() { window.location = build_link('/reports'); };

    document.querySelectorAll('.keyimg').forEach(function(element, index) {
        element.setAttribute('href', url_for_key(context.keys[index]));
        element.textContent = fmt_key(context.keys[index]).replaceAll(':', '\u200b:');
    });
}
